server {
    listen 80;
    server_name yugioh.tech;

    location /api/ {
        proxy_pass http://localhost:5002;
        include /etc/nginx/proxy_params;
        proxy_redirect off;
    }
    location / {
        proxy_pass http://localhost:5003;
        include /etc/nginx/proxy_params;
        proxy_redirect off;
    }
    location /static/ {
        alias /home/ubuntu/AirBnB_clone_v4/web_dynamic/static/;
    }
}
